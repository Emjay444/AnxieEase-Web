# Environment Variables for AnxieEase Admin Dashboard

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=anxieease-sensors.firebaseapp.com
REACT_APP_FIREBASE_DATABASE_URL=https://anxieease-sensors-default-rtdb.asia-southeast1.firebasedatabase.app
REACT_APP_FIREBASE_PROJECT_ID=anxieease-sensors
REACT_APP_FIREBASE_STORAGE_BUCKET=anxieease-sensors.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id_here

# Firebase Development Settings (optional)
REACT_APP_USE_FIREBASE_EMULATOR=false
REACT_APP_FIREBASE_EMULATOR_HOST=localhost
REACT_APP_FIREBASE_EMULATOR_PORT=9000

# Supabase Debug Mode (optional)
VITE_DEBUG_SUPABASE=false

# Development Mode Settings
NODE_ENV=development